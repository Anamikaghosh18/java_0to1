<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id = "Specification" class="car.com.example.autowire.name.Car.Specification">
        <property name="make" value = "Toyota">
        </property>
        <property name="model" value="Corola">
        </property>
    </bean>

    <bean id="myCar" class="car.com.example.autowire.name.Car.Car" autowire="byName">
        <!-- used to pass argument to a dependent -->
        <!-- constructor-arg ref="carSpecification" -->
    </bean>

</beans>